 
 <head>
      <link rel="stylesheet" href="../fontawesome-free-5.12.1-web/css/all.css">
     {{!--  <link rel="stylesheet" href="../vipermedix/css/login.css"> --}}
      <link rel="stylesheet" href="/css/login.css"/>
      <script src="../js/util.js"></script>
      <script src="../vipermedix/js/properties.js"></script>
     <script src="../vipermedix/js/login.js"></script>
 </head>
 <body class="full-cover-background" style="background-image:url(../img/medico.jpg);">
<div class="form-container">
  <p class="text-center" style="margin-top: 17px;">
     <i class="zmdi zmdi-account-circle zmdi-hc-5x"></i>
 </p>
  <h4 class="text-center all-tittles" style="margin-bottom: 30px;">inicia sesión con tu cuenta</h4>
 <form id={{login.frm_name}} action="/validateSession" method="POST"  role="form">
      <div class="group-material-login">
        <input type="text" class="material-login-control" v-model={{login.frm_emp}} name="{{login.frm_emp}}">
        <span class="highlight-login"></span>
        <span class="bar-login"></span>
        <label><i class="fa fa-hospital"></i> &nbsp; {{login.key_emp}}</label>
      </div><br>
      <div class="group-material-login">
        <input type="text" class="material-login-control"  v-model={{login.frm_usr}}>
        <span class="highlight-login"></span>
        <span class="bar-login"></span>
        <label><i class="zmdi zmdi-account"></i> &nbsp; {{login.key_user}}</label>
      </div><br>
      <div class="group-material-login">
        <input type="password" class="material-login-control" v-model={{login.frm_pas}}>
        <span class="highlight-login"></span>
        <span class="bar-login"></span>
        <label><i class="zmdi zmdi-lock"></i> &nbsp; Contraseña</label>
      </div>
      <button class="btn-login" type="submit">Ingresar al sistema &nbsp; <i class="zmdi zmdi-arrow-right"></i></button>
  </form>
</div> 
</body>
